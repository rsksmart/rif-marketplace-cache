{
  "name": "@rsksmart/rif-marketplace-cache",
  "version": "1.4.0",
  "description": "Caching server for RIF Marketplace",
  "keywords": [
    "RIF",
    "RSK"
  ],
  "homepage": "https://github.com/rsksmart/rif-marketplace-cache",
  "bugs": {
    "url": "https://github.com/rsksmart/rif-marketplace-cache/issues/"
  },
  "license": "MIT",
  "author": {
    "name": "Adam Uhlíř",
    "email": "adam@uhlir.dev"
  },
  "files": [
    "/oclif.manifest.json",
    "/lib",
    "/config/default.json5",
    "/config/custom-environment-variables.json5",
    "/bin"
  ],
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "main": "lib/index.js",
  "bin": {
    "rif-marketplace-cache": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsksmart/rif-marketplace-cache.git"
  },
  "scripts": {
    "prepack": "ts-node-dev -- node_modules/.bin/oclif-dev manifest && sed -i '' 's#\"./src/cli\"#\"./lib/cli\"#g' package.json",
    "postpack": "sed -i '' 's#\"./lib/cli\"#\"./src/cli\"#g' package.json",
    "bin": "node -r ts-node/register ./bin/run",
    "bin:watch": "ts-node-dev -- ./bin/run",
    "compile": "tsc --declaration --declarationDir types",
    "types-check": "tsc --noEmit",
    "docs": "typedoc --mode modules --excludeNotExported --readme none --excludePrivate  --tsconfig ./tsconfig.json --exclude 'src/providers/*,test/**/*' --out docs src",
    "lint": "eslint --ext .ts src/ test/",
    "release": "npm run generate-changelog && npm run compile && npm publish",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "npm run run-test \"./test/integration/**/*.spec.ts\"",
    "test:unit": "npm run run-test \"./test/unit/**/*.spec.ts\"",
    "run-test": "cross-env ALLOW_CONFIG_MUTATIONS=true NODE_ENV=test nyc --nycrc-path=./.nycrc mocha --compilers ts-node/register --require source-map-support/register --full-trace --colors --exit",
    "start": "ts-node-dev --watch -- ./src/index.ts",
    "generate-changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
